  "version": "5.0.0",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "@slixites/gatsby-plugin-google-fonts": "^1.1.1",
    "@tailwindcss/typography": "^0.4.0",
    "gatsby": "2.30.3",
    "gatsby-plugin-manifest": "2.10.0",
    "gatsby-plugin-offline": "3.8.0",
    "gatsby-plugin-postcss": "3.5.0",
    "gatsby-plugin-react-helmet": "3.8.0",
    "gatsby-plugin-sharp": "2.12.2",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-icons": "^4.1.0",
    "react-particles-js": "^3.4.1"
  },
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.2.1",
    "autoprefixer": "10.2.1",
