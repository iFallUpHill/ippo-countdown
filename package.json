    "gatsby-plugin-eslint": "2.0.8",
    "postcss": "8.2.4",
    "prettier": "2.2.1",
    "tailwindcss": "2.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iFallUpHill/ippo-countdown"
  }
